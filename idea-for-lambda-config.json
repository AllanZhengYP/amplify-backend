{
  "handler": "@aws-amplify/serverless-function",
  "definition": {
    "runtime": "node14.x",
    "handler": "index.handler",
    "src": {
      "path": "./apiRequestHandler"
    },
    "buildCommand": "tsc",
    "build": {
      "path": "./apiRequestHandler"
    }
  },
  "events": {
    "ddbTable": ["stream"],
    "restApi": ["POST /some/path", "GET /some/*"],
    "gqlApi": ["Query.myModel.myField", "Mutation.anotherModel"],
    "anotherFunction": ["invoke"],
    "storageBucket": ["OnUpdate"],
    "myAuth": ["PreSignUp", "PostConfirmation"]
  }
}
